{
	"name": "flo",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"start": "vite",
		"dev": "vite",
		"test": "jest",
		"test:watch": "jest --watch",
		"build:icons": "cd packages/icons && npm run build",
		"build:constants": "cd packages/constants && npm run build",
		"build:schemas": "cd packages/schemas && npm run build",
		"build:project": "tsc && vite build",
		"build": "npm run build:icons && npm run build:constants && npm run build:schemas && npm run build:project",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"serve": "cd json-server && npm start",
		"serve:watch": "cd json-server && npm run start:watch",
		"gen-loc": "loc gen-interface Loc -i ./src/assets/l10n/en.json -o ./src/shared/types"
	},
	"workspaces": [
		"packages/*"
	],
	"dependencies": {
		"@emotion/react": "^11.11.3",
		"@emotion/styled": "^11.11.0",
		"@flo.app/constants": "file:packages/constants",
		"@flo.app/icons": "file:packages/icons",
		"@flo.app/schemas": "file:packages/schemas",
		"@mui/icons-material": "^5.15.9",
		"@mui/lab": "^5.0.0-alpha.167",
		"@mui/material": "^5.15.9",
		"@nandn/loc": "^0.6.3",
		"@reduxjs/toolkit": "^2.2.1",
		"axios": "^1.6.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^9.1.0",
		"react-router-dom": "^6.22.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@types/react": "^18.2.55",
		"@types/react-dom": "^18.2.19",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-react": "^4.2.1",
		"eslint": "^8.56.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"jest": "^29.7.0",
		"json-server": "^0.17.4",
		"lerna": "^8.1.2",
		"nodemon": "^3.0.3",
		"typescript": "^5.2.2",
		"vite": "^5.1.0"
	}
}
